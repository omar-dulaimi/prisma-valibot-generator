{
  "name": "prisma-valibot-generator",
  "version": "1.2.0",
  "description": "Prisma generator to emit Valibot schemas from your Prisma schema",
  "repository": "https://github.com/omar-dulaimi/prisma-valibot-generator",
  "bin": {
    "prisma-valibot-generator": "lib/generator.js"
  },
  "scripts": {
    "build": "tsc",
    "exec": "npm run gen-example",
    "gen-example": "tsc && npx prisma generate",
    "gen-example:debug": "tsc && DEBUG_PRISMA_VALIBOT=1 npx prisma generate",
    "check-uncommitted": "git diff-index --quiet HEAD --",
    "package:publish": "npm update && npm run check-uncommitted && ./package.sh && cd package && npm publish",
    "test": "vitest run tests/ --reporter=verbose",
    "test:valibot": "npm run gen-example && vitest run tests/valibot-generator.test.ts",
    "test:watch": "vitest --watch",
    "test:type-check": "tsc --noEmit",
    "lint": "eslint src/ tests/ --fix",
    "format": "prettier --write src/ tests/",
    "prerelease": "npm run gen-example && npm run test:type-check",
    "release": "semantic-release",
    "release:dry": "semantic-release --dry-run",
    "release:predict": "git fetch --tags --quiet && semantic-release --dry-run --no-ci --plugins @semantic-release/commit-analyzer",
    "release:predict:notes": "git fetch --tags --quiet && semantic-release --dry-run --no-ci --plugins @semantic-release/commit-analyzer,@semantic-release/release-notes-generator",
    "ci:test": "vitest run tests/ --reporter=verbose",
    "ci:build": "npm run gen-example",
    "postrelease": "node scripts/sync-root-version.mjs"
  },
  "author": {
    "name": "Omar Dulaimi",
    "url": "https://github.com/omar-dulaimi"
  },
  "keywords": [
    "prisma",
    "prisma-client",
    "prisma-schema",
    "valibot",
    "prisma-generator",
    "prisma-valibot-generator"
  ],
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@prisma/generator-helper": "^6.17.1",
    "@prisma/internals": "^6.17.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "node-fetch": "^3.3.2",
    "prettier": "^3.6.2",
    "tslib": "^2.8.1",
    "valibot": "^0.42.1"
  },
  "devDependencies": {
    
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/node": "^24.7.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "expect-type": "^1.2.2",
    "prisma": "^6.17.1",
    "semantic-release": "^24.2.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  
  "bugs": {
    "url": "https://github.com/omar-dulaimi/prisma-valibot-generator/issues"
  },
  "homepage": "https://github.com/omar-dulaimi/prisma-valibot-generator#readme",
  "private": false
}
